# Development Environment Configuration

# Environment
ARA_ENV=development
ARA_LOG_LEVEL=DEBUG

# API Settings
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=1

# Database
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=ara_ai_dev
POSTGRES_USER=ara_dev
POSTGRES_PASSWORD=dev_password

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Security (use strong keys in production!)
JWT_SECRET_KEY=dev_jwt_secret_key_change_me
ARA_MASTER_KEY=dev_master_key_change_me

# Features
ENABLE_CORS=true
ENABLE_DOCS=true
ENABLE_METRICS=true

# Rate Limiting
RATE_LIMIT_ENABLED=false

# External APIs (add your keys)
ALPHA_VANTAGE_API_KEY=
POLYGON_API_KEY=
TWITTER_API_KEY=
REDDIT_CLIENT_ID=
REDDIT_CLIENT_SECRET=
